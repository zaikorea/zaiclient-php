{
    "name": "zaikorea/zaiclient",
    "description": "Z.Ai official client SDK",
    "autoload": {
        "psr-4": {
            "ZaiKorea\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Z.Ai Inc."
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "~7.4",
        "netresearch/jsonmapper": "~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5",
        "phpdocumentor/phpdocumentor": "*"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    }
}
